{
  "plugins": ["react-hooks"],
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"],
  "env": {
    "es6": true
  },
  "globals": {
    "document": "writable",
    "fetch": "writable",
    "localStorage": "writable",
    "window": "writable"
  },
  "rules": {
    "object-curly-newline": "off"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}
