{
  "name": "kdc-cms-api",
  "version": "0.1.0",
  "description": "API for kdc-cms",
  "main": "index.js",
  "scripts": {
    "start": "npm run ddb:wait && sls offline --port 8101 -s local",
    "start:local": "nodemon --exec \"sls offline start\" -e \"models/*.js,helpers/*.js,routes/*.js,app.js,lambda.js\"",
    "ddb:wait": "until nc -z localhost 8103; do sleep 1; done && echo \"DynamoDB started\" && sleep 2",
    "test": "mocha --watch",
    "coverage": "nyc mocha --exit",
    "deploy": "sls deploy -v -s ",
    "cleanZip": "rm -fR kdc-cms-server.zip build VERSION",
    "zip": "cd build && zip -r ../kdc-cms-server.zip *",
    "lint": "eslint . --fix"
  },
  "author": {
    "name": "Ian Dela Cruz",
    "email": "ian@kdcsoftware.com",
    "url": "https://www.kdcsoftware.com"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.550.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "kdc-cms-utils": "0.1.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "readline-sync": "^1.4.10",
    "serverless-bundle": "^1.2.5",
    "serverless-offline": "^5.12.0",
    "serverless-plugin-monorepo": "^0.8.0",
    "serverless-stage-manager": "^1.0.5",
    "supertest": "^4.0.2"
  }
}
